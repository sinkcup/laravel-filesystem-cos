{
    "name": "sinkcup/laravel-filesystem-cos-updated",
    "description": "A Cos storage filesystem for Laravel. 升级依赖版，支持 guzzle 7、Laravel 8",
    "require": {
        "php": "^7.3 | ^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "sinkcup/cos-sdk-v5-updated": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dscmall\\Flysystem\\Cos\\": "src"
        }
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Dscmall\\Flysystem\\Cos\\CosStorageServiceProvider"
            ]
        }
    },
    "conflict": {
        "dscmall/laravel-filesystem-cos": "*"
    }
}
